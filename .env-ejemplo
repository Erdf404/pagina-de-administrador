# Configuración de Base de Datos
DB_HOST=localhost   # Dirección del servidor (localhost, 127.0.0.1 o IP remota)
DB_NAME=nombre_bd   # Nombre de tu base de datos
DB_USER=usuario     # Usuario de MySQL/PostgreSQL
DB_PASS=contraseña  # Contraseña del usuario de BD

# Configuración de la Aplicación
APP_ENV=development     # Entorno: development, production o testing
APP_DEBUG=false        # true para mostrar errores, false en producción